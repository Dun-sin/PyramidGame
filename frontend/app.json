{
  "expo": {
		"name": "Pyramid Game",
		"slug": "Pyramid Game",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"scheme": "pyramidgame",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.anonymous0106.pyramidgame"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/pyramid_game_foreground.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.anonymous0106.pyramidgame"
		},
		"web": {
			"favicon": "./assets/favicon.ico",
			"bundler": "metro"
		},
		"plugins": ["@config-plugins/detox", "expo-router", "expo-font"]
	}
}
